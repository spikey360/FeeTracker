<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.ft.feetracker"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:label="@string/app_name"
    		 android:icon="@drawable/icon"
    	 >
        <activity android:name="MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
        android:name="com.ft.feetracker.AddTuitionActivity"
        android:label="@string/titleAddTuition"
        >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.ft.feetracker.MainActivity" />
    </activity>
    <activity
        android:name="com.ft.feetracker.AddPaymentActivity"
        android:label="@string/titleAddPayment"
        >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.ft.feetracker.MainActivity" />
    </activity>
    </application>
</manifest> 
